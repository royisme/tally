{
  "name": "frontend",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "bun run typecheck && vite build",
    "preview": "vite preview",
    "test": "mkdir -p .tmp && NODE_OPTIONS=\"--localstorage-file=$(pwd)/.tmp/localstorage.json\" vitest",
    "test:run": "mkdir -p .tmp && NODE_OPTIONS=\"--localstorage-file=$(pwd)/.tmp/localstorage.json\" vitest run",
    "test:e2e": "playwright test",
    "typecheck": "vue-tsc --noEmit",
    "lint": "oxlint src",
    "lint:fix": "oxlint src --fix",
    "check": "bun run typecheck && bun run lint && bun run test:run"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.18",
    "@tanstack/vue-table": "^8.21.3",
    "@vee-validate/zod": "^4.15.1",
    "@vueuse/core": "^14.1.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "echarts": "^6.0.0",
    "lucide-vue-next": "^0.561.0",
    "pinia": "^3.0.4",
    "reka-ui": "^2.6.1",
    "sass": "^1.95.0",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.18",
    "vee-validate": "^4.15.1",
    "vue": "^3.5.24",
    "vue-echarts": "^8.0.1",
    "vue-i18n": "^11.2.2",
    "vue-router": "^4.6.3",
    "vue-sonner": "^2.0.9",
    "zod": "3.25.76",
    "zod-i18n-map": "^2.27.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@types/node": "^24.10.1",
    "@vitejs/plugin-vue": "^6.0.2",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "happy-dom": "^20.0.11",
    "oxlint": "^1.32.0",
    "tw-animate-css": "^1.4.0",
    "typescript": "~5.9.3",
    "vite": "^7.2.4",
    "vitest": "^4.0.15",
    "vue-tsc": "^3.1.4"
  }
}
